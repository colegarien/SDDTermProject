<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="edu.uco.schambers.classmate.Fragments.TeacherQuestionResults">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/choicesLayout">



        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:id="@+id/display3Choice"
            android:text="Display 3 Choice Question"
            />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:id="@+id/display4Choice"
            android:layout_below="@+id/display3Choice"
            android:text="Display 4 Choice Question"
            />
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:id="@+id/display5Choice"
            android:text="Display 5 Choice Question"
            android:layout_below="@+id/display4Choice"
            />




    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/lin02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="bottom"
        android:layout_gravity="center">

    </RelativeLayout>

</FrameLayout>
