<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme" parent="android:Theme.Material.Light.DarkActionBar">
        <!-- Main theme colors -->
        <!--   your app branding color for the app bar -->
        <item name="android:colorPrimary">#1565C0</item>
        <!--   darker variant for the status bar and contextual app bars -->
        <item name="android:colorPrimaryDark">#0D47A1</item>
        <!--   theme UI controls like checkboxes and text fields -->
        <item name="android:colorAccent">#fde820</item>
        <item name="android:textColor">#1565C0</item>
        <item name="android:textColorPrimary">#000</item>
        <item name="android:buttonStyle">@style/ButtonViewStyle</item>
        <item name="android:colorButtonNormal">#1565C0</item>

    </style>

    <style name="ButtonViewStyle" parent="android:Widget.Material.Light.Button">
        <item name="android:textColor">#FFFFFF</item>
    </style>
</resources>
